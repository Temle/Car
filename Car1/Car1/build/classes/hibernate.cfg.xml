<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
    
    	
		<property name="current_session_context_class">thread</property>
    
    
        <property name="hibernate.bytecode.use_reflection_optimizer">false</property>
    
     
        <property name="hibernate.connection.driver_class">org.gjt.mm.mysql.Driver</property>
     
        <property name="hibernate.connection.password">123456</property>
        <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/car?useUnicode=true&amp;characterEncoding=utf8</property>
        <property name="hibernate.connection.username">root</property>
        <property name="hibernate.dialect">org.hibernate.dialect.MySQL5Dialect</property>
        <property name="hibernate.search.autoregister_listeners">true</property>
        <property name="hibernate.validator.apply_to_ddl">false</property>
        <property name="hibernate.show_sql">true</property>
        
        <property name="hibernate.cache.use_second_level_cache">false</property>
        
        
        <property name="hibernate.hbm2ddl.auto">update</property>
        <mapping class="com.car.domain.Personcar" />
        <mapping class="com.car.domain.User" />
        <mapping class="com.car.domain.Drivingschool" />
        <mapping class="com.car.domain.Drivingcompanion" />
        <mapping class="com.car.domain.Storereport" />
        <mapping class="com.car.domain.Notification" />
        <mapping class="com.car.domain.Parkorder" />
        <mapping class="com.car.domain.Backtalking" />
        <mapping class="com.car.domain.Storeproduct" />
        <mapping class="com.car.domain.Postconment" />
        <mapping class="com.car.domain.Postconmentreport" />
        <mapping class="com.car.domain.Conment" />
        <mapping class="com.car.domain.Drivingcorder" />
        <mapping class="com.car.domain.Subject" />
        <mapping class="com.car.domain.Storeorder" />
        <mapping class="com.car.domain.Personscore" />
        <mapping class="com.car.domain.Storenotification" />
        <mapping class="com.car.domain.Person" />
        <mapping class="com.car.domain.Park" />
        <mapping class="com.car.domain.Postreport" />
        <mapping class="com.car.domain.Post" />
        <mapping class="com.car.domain.Drivingcoach" />
        <mapping class="com.car.domain.Drivingorder" />
        <mapping class="com.car.domain.Customerservice" />
        <mapping class="com.car.domain.Point" />
        <mapping class="com.car.domain.Vip" />
        <mapping class="com.car.domain.Store" />
    </session-factory>
</hibernate-configuration>
